{
  "name": "@uraniadev/sveltekit-valibot-openapi",
  "version": "0.1.5",
  "description": "Generate an OpenAPI 3.1 specification from your SvelteKit routes and Valibot schemas.",
  "author": "uraniadev <m.isaia@urania.dev>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && bun build ./src/index.ts --outdir=dist --minify --target=node",
    "dev": "bun --watch src",
    "prepare": "bun run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/urania-dev/sveltekit-valibot-openapi.git"
  },
  "peerDependencies": {
    "@valibot/to-json-schema": "^1.0.0",
    "valibot": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.3",
    "@types/node": "^22.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.2.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.10"
  },
  "keywords": [
    "validation",
    "typescript",
    "valibot",
    "openapi",
    "openapi-3",
    "swagger",
    "scalar",
    "rest",
    "api",
    "documentation",
    "schema",
    "json-schema",
    "svelte",
    "sveltekit",
    "svelte5",
    "generator",
    "specification",
    "typed",
    "endpoint",
    "api-docs",
    "api-spec",
    "sdk",
    "rest-api"
  ]
}
